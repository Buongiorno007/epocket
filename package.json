{
  "name": "EpocketCash",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "npx react-native run-android",
    "ios": "react-native run-ios",
    "android:release": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ../",
    "android:clean": "cd android && ./gradlew clean && cd ../",
    "android_bundleRelease": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ../",
    "js-check": "eslint --print-config .eslintrc | eslint-config-prettier-check --fix",
    "prettier": "prettier --write './src/**/*.js'",
    "text-gradient": "node node_modules/react-native-text-gradient/patch-rn.js",
    "module": "rm -rf node_modules/react-native-instagram-story-share && rm -rf node_modules/react-native-maps-super-cluster && cp -r src/native_modules/ node_modules/",
    "postinstall": "npm run text-gradient && npm run module && npx jetify",
    "pod:clean": "cd ios && pod deintegrate && pod clean && pod install && cd ../",
    "watchman": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules/ && npm cache verify && npm install && npm start -- --reset-cache",
    "watchman2": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules/ && rm -rf package-lock.json && npm cache verify && npm install && npm run pod:clean && npm start -- --reset-cache",
    "xcode": "open ios/EpocketCash.xcworkspace"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.4.2",
    "@react-native-community/blur": "^3.3.1",
    "@react-native-community/netinfo": "^3.2.1",
    "bundle": "^2.1.0",
    "geolib": "^3.0.3",
    "i18n": "^0.8.3",
    "intl": "^1.2.5",
    "jail-monkey": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "native-base": "^2.12.1",
    "prop-types": "^15.7.2",
    "qs": "^6.7.0",
    "react": "16.8.6",
    "react-native": "0.59.9",
    "react-native-android-location-enabler": "^1.1.0",
    "react-native-android-open-settings": "^1.3.0",
    "react-native-animatable": "^1.3.2",
    "react-native-background-fetch": "^2.6.1",
    "react-native-background-geolocation": "^3.0.9",
    "react-native-background-timer": "^2.1.1",
    "react-native-barcode-builder": "^1.0.5",
    "react-native-camera": "git+https://git@github.com/react-native-community/react-native-camera.git",
    "react-native-collapsible": "^1.4.0",
    "react-native-cookies": "^3.3.0",
    "react-native-fast-image": "^6.0.3",
    "react-native-fbsdk": "^0.10.1",
    "react-native-firebase": "^5.4.3",
    "react-native-fs": "^2.13.3",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-i18n": "^2.0.15",
    "react-native-image-picker": "^0.28.1",
    "react-native-instagram-story-share": "Jobeso/react-native-instagram-story-share#3e03e04",
    "react-native-linear-gradient": "2.5.4",
    "react-native-maps": "0.24.2",
    "react-native-maps-super-cluster": "^1.5.0",
    "react-native-masked-text": "^1.12.4",
    "react-native-material-textfield": "^0.12.0",
    "react-native-mauron85-background-geolocation": "git+https://github.com/kruyvanna/react-native-background-geolocation.git",
    "react-native-permissions": "^1.1.1",
    "react-native-qrcode-svg": "^5.1.2",
    "react-native-radial-gradient": "^1.0.5",
    "react-native-share": "git+https://github.com/react-native-community/react-native-share.git",
    "react-native-sound": "^0.11.0",
    "react-native-svg": "^9.5.3",
    "react-native-swipe-gestures": "^1.0.3",
    "react-native-text-gradient": "0.1.6",
    "react-native-webview": "^5.12.1",
    "react-navigation": "^3.11.1",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "remote-redux-devtools": "^0.5.16",
    "rn-fetch-blob": "0.10.15"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "babel-eslint": "^10.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-native": "^3.7.0",
    "jetifier": "^1.6.1",
    "metro-react-native-babel-preset": "0.54.1",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "redux-devtools-extension": "^2.13.8",
    "schedule": "^0.5.0"
  }
}
